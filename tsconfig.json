{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "lib": ["es2022", "dom"],
      "declaration": false,
      "sourceMap": false,
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }